

<h3> Student Form</h3>

<%= form_tag students_path do %>

  <label for=""> First Name: </label>
    # to name the param I only need to pass a string
  <%= text_area_tag :'first_name' %> <br>

  <label for=""> Last Name: </label>
  <%= text_area_tag :'last_name' %> <br>

  <%= submit_tag "Submit Student" %>
<% end %>



<%= params.inspect %>
